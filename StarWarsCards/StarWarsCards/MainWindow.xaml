<Window x:Class="StarWarsCards.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StarWarsCards"
        mc:Ignorable="d"
        Title="MainWindow" Height="253" Width="236" >
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="200*"/>
            <RowDefinition Height="20*"/>
        </Grid.RowDefinitions>
        <ListBox Name="cardsStarwars" Grid.Row="0">
            
        </ListBox>
        <StackPanel Orientation="Horizontal" Grid.Row="1">
            <TextBox x:Name="page" Margin="0 0 0 0" Width="150"/>
            <Button x:Name="load" Content="Загрузить" Margin="6 0 0 0" Width="80" Click="Load"/>
        </StackPanel>
    </Grid>
</Window>
